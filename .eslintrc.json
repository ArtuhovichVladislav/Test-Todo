{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  },
  "env": {
    "es6": true,
    "react-native/react-native": true
  },
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier", "babel", "react", "react-native"],
  "rules": {
    "prettier/prettier": ["error"]
  }
}
